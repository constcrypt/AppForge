{
	"name": "@rbxts/app-forge",
	"version": "0.4.2",
	"description": "An App Manager for react",
	"main": "out/init.lua",
	"packageManager": "bun@1.3.1",
	"scripts": {
		"build": "rbxtsc",
		"dev": "rbxtsc -w --type game --rojo test.project.json",
		"login": "bunx npm login --auth-type=legacy"
	},
	"keywords": [
		"roblox-ts",
		"react"
	],
	"author": "loner1536",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Loner1536/AppForge"
	},
	"bugs": {
		"url": "https://github.com/Loner1536/AppForge/issues"
	},
	"types": "out/index.d.ts",
	"files": [
		"out",
		"!**/*.tsbuildinfo",
		"!**/*.spec.lua",
		"!**/*.spec.d.ts"
	],
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.7",
		"@rbxts/compiler-types": "3.0.0-types.0",
		"@rbxts/react": "17.3.0-alpha.1",
		"@rbxts/react-roblox": "17.3.0-alpha.1",
		"@rbxts/types": "^1.0.891",
		"@typescript-eslint/eslint-plugin": "^8.46.4",
		"@typescript-eslint/parser": "^8.46.4",
		"roblox-ts": "3.0.0",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"@rbxts/charm": "^0.10.0",
		"@rbxts/loners-pretty-react-hooks": "^0.2.9",
		"@rbxts/react-charm": "^0.3.0",
		"@rbxts/ripple": "^0.9.3",
		"@rbxts/services": "^1.6.0",
		"@rbxts/set-timeout": "^1.1.2"
	},
	"peerDependencies": {
		"@rbxts/loners-pretty-react-hooks": "*",
		"@rbxts/react-roblox": "*",
		"@rbxts/react": "*"
	}
}
